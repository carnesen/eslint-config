{
	"name": "eslint-config-carnesen",
	"version": "12.0.0",
	"description": "ESLint configurations for @carnesen projects",
	"main": ".eslintrc.json",
	"engines": {
		"node": ">=20.0.0"
	},
	"scripts": {
		"build": "./scripts/build.sh",
		"lint": "eslint --ext .js,.jsx,.ts,.tsx examples",
		"fix": "npm run lint -- --fix",
		"release": "carnesen-dev release --semverBump",
		"release:major": "npm run release -- major",
		"release:minor": "npm run release -- minor",
		"test": "npm run lint && npm run build",
		"upgrade": "npm-check-updates --upgrade"
	},
	"dependencies": {
		"@typescript-eslint/eslint-plugin": "^7.8.0",
		"@typescript-eslint/parser": "^7.8.0",
		"eslint-config-airbnb": "^19.0.4",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-import": "^2.29.1",
		"eslint-plugin-jsx-a11y": "^6.8.0",
		"eslint-plugin-prettier": "^5.1.3",
		"eslint-plugin-react": "^7.34.1",
		"eslint-plugin-react-hooks": "^4.6.2",
		"prettier": "^3.2.5"
	},
	"peerDependencies": {
		"eslint": "^8.57.0",
		"typescript": ">=5.0.0"
	},
	"devDependencies": {
		"@carnesen/dev": "^0.4.0",
		"@types/node": "^20.12.8",
		"@types/react": "18.3.1",
		"eslint": "^8.57.0",
		"npm-check-updates": "^16.14.20",
		"react": "18.3.1",
		"typescript": "5.4.5"
	},
	"files": [
		".eslintrc.json",
		"base.eslintrc.json",
		"node.eslintrc.json",
		"react.eslintrc.json"
	],
	"repository": "github:carnesen/eslint-config",
	"author": "Chris Arnesen",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/carnesen/eslint-config/issues"
	},
	"homepage": "https://github.com/carnesen/eslint-config#readme"
}
